local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Boom Shrimp Games!",
   Icon = 0, 
   LoadingTitle = "Go for it!",
   LoadingSubtitle = "by Averages",
   Theme = "Ocean",

   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false, 

   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "Big Hub"
   },

   Discord = {
      Enabled = false,
      Invite = "noinvitelink",
      RememberJoins = true
   },

   KeySystem = false,
   KeySettings = {
      Title = "Untitled",
      Subtitle = "Key System",
      Note = "No method of obtaining the key is provided", 
      FileName = "Key", 
      SaveKey = true, 
      GrabKeyFromSite = false, 
      Key = {"Hello"} 
   }
})

local Tab = Window:CreateTab("Auto-Do", 4483362458) 

local Section = Tab:CreateSection("Lol ez")

local Button = Tab:CreateButton({
   Name = "Complete cookie",
   Callback = function()
        local args = {
    [1] = "Success",
    [2] = true
}

game:GetService("ReplicatedStorage").Remotes.Events.EffectsEvent:FireServer(unpack(args))
   end,
})

local Button = Tab:CreateButton({
   Name = "Pull rope",
   Callback = function()
        game:GetService("ReplicatedStorage").Remotes.Events.PullRope:FireServer()
   end,
})

Rayfield:LoadConfiguration()
